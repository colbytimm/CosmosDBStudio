<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:CosmosDBStudio"
                    xmlns:vm="clr-namespace:CosmosDBStudio.ViewModel;assembly=CosmosDBStudio.ViewModel"
                    xmlns:v="clr-namespace:CosmosDBStudio.View">
    <DataTemplate DataType="{x:Type vm:AccountEditorViewModel}">
        <v:AccountEditorView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:DocumentEditorViewModel}">
        <v:DocumentEditorView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:ContainerPickerViewModel}">
        <v:ContainerPickerView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:DatabaseEditorViewModel}">
        <v:DatabaseEditorView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:ContainerEditorViewModel}">
        <v:ContainerEditorView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:AboutViewModel}">
        <v:AboutView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:TextPromptViewModel}">
        <v:TextPromptView />
    </DataTemplate>
</ResourceDictionary>