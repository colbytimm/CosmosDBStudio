<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:CosmosDBStudio"
                    xmlns:vm="clr-namespace:CosmosDBStudio.ViewModel;assembly=CosmosDBStudio.ViewModel"
                    xmlns:v="clr-namespace:CosmosDBStudio.View"
                    xmlns:dialogs="clr-namespace:CosmosDBStudio.ViewModel.Dialogs;assembly=CosmosDBStudio.ViewModel">
    <DataTemplate DataType="{x:Type dialogs:AccountEditorViewModel}">
        <v:AccountEditorView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type dialogs:DocumentEditorViewModel}">
        <v:DocumentEditorView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type dialogs:ContainerPickerViewModel}">
        <v:ContainerPickerView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type dialogs:DatabaseEditorViewModel}">
        <v:DatabaseEditorView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type dialogs:ContainerEditorViewModel}">
        <v:ContainerEditorView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:AboutViewModel}">
        <v:AboutView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type dialogs:TextPromptViewModel}">
        <v:TextPromptView />
    </DataTemplate>
</ResourceDictionary>