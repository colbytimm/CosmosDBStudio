<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=System.Runtime">

    <BooleanToVisibilityConverter x:Key="boolToVisibility" />

    <sys:Boolean x:Key="True">True</sys:Boolean>
    <sys:Boolean x:Key="False">False</sys:Boolean>

    <FontFamily x:Key="CodeFont">Cascadia Code, Consolas, Courier New</FontFamily>
    <FontFamily x:Key="FontAwesome">pack://application:,,,/Resources/#Font Awesome 5 Free Solid</FontFamily>

    <sys:String x:Key="fa-play">&#xF04B;</sys:String>
    <sys:String x:Key="fa-at">&#xF1FA;</sys:String>
    <sys:String x:Key="fa-trash-alt">&#xF2ED;</sys:String>
    <sys:String x:Key="fa-plus">&#xF067;</sys:String>
    <sys:String x:Key="fa-edit">&#xF044;</sys:String>
    <sys:String x:Key="fa-save">&#xF0C7;</sys:String>
    <sys:String x:Key="fa-redo-alt">&#xF2F9;</sys:String>
    <sys:String x:Key="fa-times">&#xF00D;</sys:String>

</ResourceDictionary>